#====================================================
${CONSTITUENT}:: ${CINTFILE}
	@echo "------> ${CONSTITUENT} ok"

VPATH=../mcRootData/
${CINTFILE}:: ${root_headers}
	@echo " running rootcint to generate ${CINTFILE} "
	cd ../${package}/ ; export LD_LIBRARY_PATH=${ROOT_PATH}/lib;${ROOT_PATH}/bin/rootcint -f ${CINTFILE} -c ${root_headers}
#==================================================== 
